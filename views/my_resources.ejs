<!DOCTYPE html>
<html lang="en">

<%- include("./partials/head") %>

  <body>
    <%- include("./partials/navbar") %>

      <h1>My POSTS</h1>
      <%- include("./partials/posts") %>


        <h1>LIKED POSTS</h1>
        <% if (likeObject.length) {%>
          <div class="row">
            <% for (let post of likeObject) { %>
              <div class="col-lg-4 col-md-6 mb-4">
                <div class="card h-100">
                  <a href="/post_details/<%= post.id %>/"><img class="card-img-top" src="<%= post.image_url %>"
                      alt="Post Image" /></a>
                  <div class="card-body">
                    <h4 class="card-title">
                      <a href="/post_details/<%= post.id %>/">
                        <%= post.title %>
                      </a>
                    </h4>
                    <p class="card-text">
                      <%= post.post_description %>
                    </p>
                  </div>
                  <div class="card-footer">
                  </div>
                </div>
              </div>
              <% } %>
          </div>
          <% } else { %>
            <h3>NO Liked posts :(( </h3>
            <% } %>
              <!-- Bootstrap core JavaScript -->
              <scri pt src="vendor/jquery/jquery.min.js">
                </script>
                <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  </body>

</html>
